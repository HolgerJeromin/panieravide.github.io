{
	"type": "Styles",
	"styles": [
		{
			"type": "Style",
			"description": "Indoor passages ('OR' example for values)",
			"onTags": [ { "indoor": "area|corridor" } ],
			"style": { "color": "#AAAAAA", "opacity": 1, "weight": 0.5, "fillColor": "#EEEEEE", "fillOpacity": 1 }
		},
		{
			"type": "Style",
			"description": "Indoor rooms",
			"onTags": [ { "indoor": "room|wall" } ],
			"style": { "color": "#AAAAAA", "opacity": 1, "weight": 0.5, "fillColor": "#E0E0E0", "fillOpacity": 1 }
		},
		{
			"type": "Style",
			"description": "Door nodes ('OR' example for tags)",
			"onTags": [ { "door": "*" }, { "entrance": "*" } ],
			"style": { "icon": "img/io.svg" }
		},
		{
			"type": "Style",
			"description": "Amenities (default)",
			"onTags": [ { "amenity": "*" } ],
			"style": { "color": "black", "fillColor": "#EC989A" }
		},
		{
			"type": "Style",
			"description": "Ticket validator",
			"onTags": [ { "amenity": "ticket_validator" } ],
			"style": { "icon": "img/ticket_valid.svg" }
		},
		{
			"type": "Style",
			"description": "Ticket vending machine ('AND' example for tags)",
			"onTags": [ { "amenity": "vending_machine", "vending": "public_transport_tickets" } ],
			"style": { "icon": "img/ticket_vending.svg" }
		},
		{
			"type": "Style",
			"description": "Buildings",
			"onTags": [ { "building": "*" } ],
			"style": { "color": "#ADA5A5", "opacity": 1, "weight": 0.5, "fillColor": "#D4CACA", "fillOpacity": 1 }
		},
		{
			"type": "Style",
			"description": "Highways (default)",
			"onTags": [ { "highway": "*" } ],
			"style": { "color": "#F98072", "opacity": 1 }
		},
		{
			"type": "Style",
			"description": "Stairs",
			"onTags": [ { "highway": "steps" } ],
			"style": { "color": "#F98072", "opacity": 1, "weight": 10, "lineCap": "butt", "dashArray": "2,1" }
		},
		{
			"type": "Style",
			"description": "Conveying stairs",
			"onTags": [ { "highway": "steps", "conveying": "yes|forward|backward" } ],
			"style": { "color": "#848484", "opacity": 1, "weight": 10, "lineCap": "butt", "dashArray": "3,1" }
		},
		{
			"type": "Style",
			"description": "Subway rails",
			"onTags": [ { "railway": "subway" } ],
			"style": { "color": "#888888", "opacity": 1, "weight": 10, "lineCap": "butt", "dashArray": "5,5" }
		}
	]
}